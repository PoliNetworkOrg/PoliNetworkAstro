---
import Default from "../../layouts/Default.astro";
import Homepage from "../../layouts/Homepage.astro";
import { Languages, MultilangString } from "../../utils/types";

export async function getStaticPaths() {
  console.log(Languages.values())
    return Languages.values().map((language) => {
    return {
      params: { lang: language },
      props: { language },
    };
  });
}

const { language } = Astro.props;
---

<Homepage language={ language } info={ {
  title: ["ciao", "hello"],
  motto: ["ciao", "hello"],
  groups_title: ["ciao", "hello"],
}}/>
